        <sys:String>GOST Common</sys:String>
        <sys:String>Times New Roman</sys:String>
        <sys:String>Segoe UI</sys:String>
    </col:ArrayList>
    
    <col:ArrayList x:Key="sizeFonts">
        <sys:String>8</sys:String>
